<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bobo.test.mapper.ClassifyMapper">

    <insert id="insertClassify">
		insert into `classify`
		values(#{id},#{name},#{creationTime});
	</insert>

	<select id="list" resultType="com.bobo.test.pojo.Classify">
		select * from `classify`
	</select>
	<update id="update">
		UPDATE `classify` SET `name` = #{name} WHERE id = #{id}
	</update>
</mapper>