<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bobo.test.mapper.ThumbMapper">

    <insert id="insert">
		insert into `thumb`
		values(#{id},#{userId},#{ikonId},#{thumbTime});
	</insert>
	<select id="getThumb" resultType="Integer">
		SELECT id
		FROM `thumb`
		WHERE user_id = #{userId}
		AND ikon_id = #{ikonId}
	</select>

	<delete id="unThumb">
		Delete FROM  thumb where id = #{id}
	</delete>
</mapper>