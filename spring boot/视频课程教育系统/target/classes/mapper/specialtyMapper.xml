<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bobo.test.mapper.specialtyMapper">

    <select id="querySpecialty" resultType="specialty">
        select * from specialty
    </select>

    <select id="querySpecialtyById" resultType="specialty">
        select * from specialty where  id = #{id}
    </select>

</mapper>