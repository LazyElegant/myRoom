<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bobo.test.mapper.ScienceMapper">

    <select id="queryScienceById" parameterType="int" resultType="science">
        select * from science where id = #{id}
    </select>

    <select id="queryScienceAll"  resultType="science">
        select * from science
    </select>
</mapper>