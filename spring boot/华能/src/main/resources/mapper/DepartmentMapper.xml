<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bobo.test.mapper.DepartmentMapper">

    <select id="listDepartment"  resultType="com.bobo.test.pojo.Department">
        SELECT
            id,
            name,
            creat_time `creatTime`
        FROM department
    </select>

</mapper>