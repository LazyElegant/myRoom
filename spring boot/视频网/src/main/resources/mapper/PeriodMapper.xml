<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bobo.test.mapper.PeriodMapper">
   <insert id="addPeriod" >
       insert into `period`
       VALUES (#{id},#{create_time},#{period},#{user_id},#{video_id})
   </insert>

    <select id="getPeriod" resultType="Period">
        select *
        FROM `period`
        WHERE user_id = #{id}
        ORDER BY create_time desc;
    </select>

</mapper>